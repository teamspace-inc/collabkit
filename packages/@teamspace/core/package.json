{
  "version": "0.1.19",
  "name": "@tldraw/core",
  "description": "The TLDraw core renderer and utilities.",
  "author": "@steveruizok",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tldraw/core.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "canvas",
    "zoom",
    "drawing",
    "whiteboard"
  ],
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "start:simple": "yarn start",
    "start:advanced": "yarn start",
    "start": "node scripts/dev & yarn types:dev",
    "build": "node scripts/build && yarn types:build && node scripts/copy-readme",
    "types:dev": "tsc -w --p tsconfig.build.json",
    "types:build": "tsc -p tsconfig.build.json && tsconfig-replace-paths -p tsconfig.build.json",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "printWidth": 100
  },
  "dependencies": {
    "@tldraw/intersect": "^1.2.5",
    "@tldraw/vec": "^1.2.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@use-gesture/react": "^10.1.3",
    "fast-deep-equal": "^3.1.3",
    "nanoid": "^3.1.30"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react-dom": "^18.0.0",
    "tsconfig-replace-paths": "^0.0.14"
  }
}
