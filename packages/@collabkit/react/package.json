{
  "name": "@collabkit/react",
  "version": "0.9.6",
  "type": "module",
  "files": [
    "dist/collabkit-react.d.ts",
    "dist/collabkit-react.js",
    "dist/collabkit-react.umd.cjs",
    "dist/style.css"
  ],
  "types": "./dist/collabkit-react.d.ts",
  "main": "./dist/collabkit-react.umd.cjs",
  "module": "./dist/collabkit-react.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    ".": {
      "types": "./dist/collabkit-react.d.ts",
      "import": "./dist/collabkit-react.js",
      "require": "./dist/collabkit-react.umd.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "ladle serve",
    "build": "yarn clean && tsc && vite build && rollup --config rollup.config.js && yarn check-build",
    "check-build": "yarn tsc --noEmit --esModuleInterop dist/collabkit-react.d.ts",
    "clean": "rimraf dist types *.tsbuildinfo",
    "preview": "vite preview",
    "prepublishOnly": "yarn build",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@collabkit/client": "*",
    "@collabkit/colors": "*",
    "@collabkit/core": "*",
    "@collabkit/editor": "*",
    "@floating-ui/react": "^0.18.0",
    "@ladle/react": "^2.6.0",
    "@lexical/clipboard": "0.7.5",
    "@lexical/markdown": "^0.7.5",
    "@lexical/react": "^0.7.5",
    "@lexical/utils": "0.7.5",
    "@playwright/test": "^1.28.1",
    "@radix-ui/react-use-callback-ref": "1.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/sync-fetch": "^0.4.0",
    "@vanilla-extract/css": "^1.9.5",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.3.0",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "@vitejs/plugin-react": "^2.0.1",
    "date-fns": "^2.28.0",
    "deepmerge": "^4.2.2",
    "escape-string-regexp": "^5.0.0",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^9.9.1",
    "has": "^1.0.3",
    "lexical": "0.7.5",
    "nanoid": "^4.0.0",
    "phosphor-react": "^1.4.1",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intersection-observer": "^9.4.0",
    "react-merge-refs": "^2.0.1",
    "rehype-react": "^7.1.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "rollup": "3.15.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-visualizer": "^5.8.0",
    "start-server-and-test": "^1.14.0",
    "unified": "^10.1.2",
    "valtio": "^1.10.1",
    "vite": "^4.1.1",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.28.1",
    "react-virtuoso": "^4.1.0",
    "jwt-decode": "^3.1.2"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  }
}
