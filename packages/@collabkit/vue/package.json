{
  "name": "@collabkit/vue",
  "version": "1.0.0-beta.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/collabkit-vue.d.ts",
  "main": "./dist/collabkit-vue.umd.cjs",
  "module": "./dist/collabkit-vue.js",
  "exports": {
    ".": {
      "types": "./dist/collabkit-vue.d.ts",
      "import": "./dist/collabkit-vue.js",
      "require": "./dist/collabkit-vue.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn clean && run-p build-types build-only",
    "clean": "rimraf dist types",
    "prepublishOnly": "yarn build",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "build-types": "vue-tsc && rollup --config rollup.config.js"
  },
  "peerDependencies": {
    "@collabkit/react": "*",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "@types/node": "^16.11.47",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.1.0",
    "date-fns": "^2.28.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "2.77.3",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-visualizer": "^5.8.0",
    "typescript": "~4.7.4",
    "vite": "^4.1.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.5"
  }
}
